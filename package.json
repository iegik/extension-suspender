{
  "name": "tab-suspender-extension",
  "version": "1.0.0",
  "description": "A browser extension that suspends inactive tabs to save memory and CPU usage",
  "main": "sw.js",
  "scripts": {
    "test": "npm run test:puppeteer",
    "test:debug": "npm run test:puppeteer:debug",
    "test:cypress": "cypress run",
    "test:cypress:debug": "DEBUG=cypress:* npx cypress run --headed --no-exit --browser electron -s cypress/e2e/tab-suspender.cy.js",
    "test:cypress:open": "cypress open",
    "test:puppeteer": "jest --config puppeteer/jest.config.js",
    "test:puppeteer:debug": "jest --config puppeteer/jest.config.js --testPathPattern=working-extension --verbose",
    "test:puppeteer:simple": "jest --config puppeteer/jest.config.js --testPathPattern=simple-extension --verbose",
    "test:puppeteer:watch": "jest --config puppeteer/jest.config.js --watch",
    "test:puppeteer:coverage": "jest --config puppeteer/jest.config.js --coverage",
    "build": "make build",
    "build:firefox": "make firefox && make build",
    "build:chrome": "make chrome && make build"
  },
  "keywords": [
    "browser-extension",
    "tab-suspender",
    "memory-optimization",
    "chrome-extension",
    "firefox-addon"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.0",
    "dotenv": "^17.2.0",
    "jest": "^29.7.0",
    "puppeteer": "^24.14.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
