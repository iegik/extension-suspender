{
  "name": "tab-suspender-extension",
  "version": "1.0.4",
  "description": "A browser extension that suspends inactive tabs to save memory and CPU usage",
  "main": "sw.js",
  "scripts": {
    "test": "npm run test:unified",
    "test:unified": "npm run test:puppeteer && npm run test:cypress",
    "test:unified:puppeteer": "npm run test:puppeteer",
    "test:unified:cypress": "npm run test:cypress",
    "test:debug": "npm run test:puppeteer:debug",
    "test:cypress": "cypress run",
    "test:cypress:debug": "DEBUG=cypress:* npx cypress run --headed --no-exit --browser electron -s test/cypress-entry.js",
    "test:cypress:open": "cypress open",
    "test:puppeteer": "jest --config jest.config.js test/puppeteer-entry.test.js",
    "test:puppeteer:debug": "jest --config jest.config.js test/puppeteer-entry.test.js --verbose",
    "test:puppeteer:simple": "jest --config puppeteer/jest.config.js --testPathPattern=simple-extension --verbose",
    "test:puppeteer:watch": "jest --config jest.config.js --watch",
    "test:puppeteer:coverage": "jest --config jest.config.js --coverage",
    "build": "make build",
    "build:firefox": "make firefox && make build",
    "build:chrome": "make chrome && make build"
  },
  "keywords": [
    "browser-extension",
    "tab-suspender",
    "memory-optimization",
    "chrome-extension",
    "firefox-addon"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^14.5.4",
    "dotenv": "^17.2.1",
    "jest": "^29.7.0",
    "puppeteer": "^24.16.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "web-ext": "^8.9.0"
  },
  "overrides": {
    "tmp": "0.2.5"
  }
}
